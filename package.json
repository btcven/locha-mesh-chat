    "react-navigation-redux-helpers": "^3.0.2",
    "react-navigation-tabs": "^1.2.0",
    "react-redux": "7.1.3",
    "readable-stream": "1.0.33",
    "realm": "^5.0.4",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "0.10.15",
    "stream-browserify": "^1.0.0",
    "string_decoder": "~0.10.25",
    "styled-components": "^4.4.0"
  },
  "resolutions": {
    "native-base/react-native-keyboard-aware-scroll-view": "https://github.com/APSL/react-native-keyboard-aware-scroll-view#v0.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^26.6.3",
    "better-docs": "^1.3.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-native": "^3.8.1",
    "jest": "^26.6.3",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "js-sha256": "^0.9.0",
    "jsdoc": "^3.6.3",
    "metro-react-native-babel-preset": "^0.59.0",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|native-base|react-native-image-pan-zoom|react-native-image-zoom-viewer|react-native-qrcode-svg|react-native-camera|react-native-modal|react-native-qrcode-scanner|react-native-material-menu|react-native-iphone-x-helper|react-native-easy-grid|native-base-shoutem-theme|@shoutem/animation|@shoutem/ui|react-native-image-crop-picker|tcomb-form-native|react-navigation|@codler|react-native-vector-icons|@react-native-picker|react-native-animatable|react-native-drawer|react-native-permissions)/)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/testSetup.js",
      "<rootDir>/node_modules/jest-enzyme/lib/index.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    }
  }
}
